{
  "name": "pagebuilder-poc",
  "version": "0.1.0",
  "private": true,
  "homepage": "backoffice/",
  "dependencies": {
    "@appquality/appquality-design-system": "^1.0.44",
    "@appquality/craft-blocks": "^0.1.27",
    "@craco/craco": "^6.4.3",
    "@craftjs/core": "^0.2.0-beta.5",
    "@reduxjs/toolkit": "^1.8.3",
    "husky": "^7.0.4",
    "lzutf8": "^0.6.0",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-bootstrap-icons": "^1.7.2",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.2.0",
    "react-transition-group": "^4.4.2",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1",
    "styled-components": "^5.3.0",
    "typescript": "^4.5.5",
    "uuid": "^8.3.2",
    "web-vitals": "^1.1.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "env": {
      "browser": true
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "move": "mkdir -p build/backoffice/ &&  mv build/* build/backoffice/ ; true",
    "prepare": "npx husky install",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "generate-schema": ". ./.env.development; npx openapi-typescript $REACT_APP_API_URL/reference/ --output src/utils/schema.ts ",
    "start": "craco start",
    "build": "react-scripts build && npm run move",
    "test": "react-scripts test",
    "prettier": "npx prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "translate": "i18next-scanner",
    "generate-api": ". ./.env.development; API_URL=$REACT_APP_API_URL/reference/ npx @rtk-query/codegen-openapi src/services/tryberApi/config.ts"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rtk-query/codegen-openapi": "^1.0.0-alpha.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^12.1.10",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-transition-group": "^4.4.4",
    "@types/styled-components": "^5.1.23",
    "@types/uuid": "^8.3.4",
    "jest-canvas-mock": "^2.4.0",
    "msw": "^0.47.4",
    "openapi-typescript": "^5.2.0",
    "react-scripts": "^4.0.3",
    "ts-node": "^10.9.1"
  }
}
